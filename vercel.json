{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/.*",
      "dest": "/index.js"
    }
  ],
  "env": {
    "PRISMA_FORCE_NAPI": "true"
  },
  "functions": {
    "addPrismaClient": {
      "runtime": "@vercel/node@1.22.1",
      "memory": 1024,
      "maxDuration": 10,
      "includeFiles": "node_modules/.prisma/**"
    }
  }
}